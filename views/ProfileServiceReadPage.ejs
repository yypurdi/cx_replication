                <tr>
                    <td><a><%=rec.profile_id%></a></td>
                    <td><a><%=rec.co_id%></a></td>
                    <td><a><%=rec.sncode%></a></td>
                    <td><a><%=rec.histno%></a></td>
                    <td><a><%=rec.status%></a></td>
                    <td><a><%=rec.reason%></a></td>
                    <td><a><%=rec.transactionno%></a></td>
                    <td><a><%=dateFormat(rec.valid_from_date,'mmm-dd-yyyy h:MM:ss TT')%></a></td>
                    <td><a><%=dateFormat(rec.entry_date,'mmm-dd-yyyy h:MM:ss TT')%></a></td>
                    <td><a><%=rec.request_id%></a></td>
                    <td><a><%=rec.rec_version%></a></td>
                    <td><a><%=rec.user_reason%></a></td>
                    
                    <td><a href="/readPrServStatusHist?profileId=<%=rec.profile_id%>" style="color: green"><span class="glyphicon glyphicon-open"></span></a></td>
                    <% if(session.user_acl=='ROLE_ADMIN') { %>
                    <td><a href="/writePrServStatusHist?profileId=<%=rec.profile_id%>" style="color: green"><span class="glyphicon glyphicon-edit"></span></a></td>
                    <td><a href="/deletePrServStatusHist?profileId=<%=rec.profile_id%>" style="color: green"><span class="glyphicon glyphicon-trash"></span></a></td>
                    <% } %>
                </tr>
                <% } %>
            </table>        
	    <button type="submit" class="btn btn-default"><a href="/newPrServStatusHist">Add</a></button>
	    <button type="submit" class="btn btn-default">Clear</button>
	    </div>	    
        </div>            
        
    </div>

</section>
</div>
</div>
