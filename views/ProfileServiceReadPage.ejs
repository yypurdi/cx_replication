        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><h4><%=resource.Heading%></h4></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="prServSpcodeHist-tab" data-toggle="tab" href="#prServSpcodeHist" role="tab" aria-controls="prServSpcodeHist" aria-selected="true"><h4><%=resource.PrServSpcodeHist.Heading%></h4></a>
