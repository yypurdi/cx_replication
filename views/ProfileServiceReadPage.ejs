                    </tr><tr>
                        <td><%=resource.StatusHistno.label%></td>
                        <td><%=record.status_histno%></td>						
                    </tr><tr>
                        <td><%=resource.EntryDate.label%></td>
                        <td><%=dateFormat(record.entry_date,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.ChannelNum.label%></td>
                        <td><%=record.channel_num%></td>						
                    </tr><tr>
                        <td><%=resource.OvwAccFirst.label%></td>
                        <td><%=record.ovw_acc_first%></td>						
                    </tr><tr>
                        <td><%=resource.DateBilled.label%></td>
                        <td><%=dateFormat(record.date_billed,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.SnClass.label%></td>
                        <td><%=record.sn_class%></td>						
                    </tr><tr>
                        <td><%=resource.OvwSubscr.label%></td>
