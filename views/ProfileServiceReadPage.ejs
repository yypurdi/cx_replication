                        <td><%=record.delete_flag%></td>						
                    </tr><tr>
                        <td><%=resource.RecVersion.label%></td>
                        <td><%=record.rec_version%></td>						
                    </tr><tr>
                        <td><%=resource.AttribHistno.label%></td>
                        <td><%=record.attrib_histno%></td>						
                    </tr><tr>
                        <td><%=resource.AttribHistnoNnp.label%></td>
                        <td><%=record.attrib_histno_nnp%></td>						
                    </tr>
                
                </table>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <button type="submit" class="btn btn-default"><a href="/writeProfileService?profileId=<%=record.profile_id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteProfileService?profileId=<%=record.profile_id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
	<div class="tab-pane fade" id="prServSpcodeHist" role="tabpanel" aria-labelledby="prServSpcodeHist-tab">
            <div style="width:75%">
            <table class="table table-striped">
                <tr class="heading-tr">
                    <th><%=resource.PrServSpcodeHist.ProfileId.header%></th>
                    <th><%=resource.PrServSpcodeHist.CoId.header%></th>
                    <th><%=resource.PrServSpcodeHist.Sncode.header%></th>
                    <th><%=resource.PrServSpcodeHist.Histno.header%></th>
                    <th><%=resource.PrServSpcodeHist.Spcode.header%></th>
                    <th><%=resource.PrServSpcodeHist.Transactionno.header%></th>
                    <th><%=resource.PrServSpcodeHist.ValidFromDate.header%></th>
                    <th><%=resource.PrServSpcodeHist.EntryDate.header%></th>
                    <th><%=resource.PrServSpcodeHist.RequestId.header%></th>
                    <th><%=resource.PrServSpcodeHist.RecVersion.header%></th>
                                        
                    <th>View</th>
                    <% if(session.user_acl=='ROLE_ADMIN') { %>
                    <th>Edit</th>
                    <th>Delete</th>
                    <% } %>
                </tr>
                <% for(let i in record.prServSpcodeHist){ let rec = record.prServSpcodeHist[i] %>
                <tr>
                    <td><a><%=rec.profile_id%></a></td>
                    <td><a><%=rec.co_id%></a></td>
                    <td><a><%=rec.sncode%></a></td>
                    <td><a><%=rec.histno%></a></td>
                    <td><a><%=rec.spcode%></a></td>
                    <td><a><%=rec.transactionno%></a></td>
                    <td><a><%=dateFormat(rec.valid_from_date,'mmm-dd-yyyy h:MM:ss TT')%></a></td>
                    <td><a><%=dateFormat(rec.entry_date,'mmm-dd-yyyy h:MM:ss TT')%></a></td>
                    <td><a><%=rec.request_id%></a></td>
                    <td><a><%=rec.rec_version%></a></td>
                    
                    <td><a href="/readPrServSpcodeHist?profileId=<%=rec.profile_id%>" style="color: green"><span class="glyphicon glyphicon-open"></span></a></td>
                    <% if(session.user_acl=='ROLE_ADMIN') { %>
                    <td><a href="/writePrServSpcodeHist?profileId=<%=rec.profile_id%>" style="color: green"><span class="glyphicon glyphicon-edit"></span></a></td>
                    <td><a href="/deletePrServSpcodeHist?profileId=<%=rec.profile_id%>" style="color: green"><span class="glyphicon glyphicon-trash"></span></a></td>
                    <% } %>
                </tr>
                <% } %>
            </table>        
	    <button type="submit" class="btn btn-default"><a href="/newPrServSpcodeHist">Add</a></button>
	    <button type="submit" class="btn btn-default">Clear</button>
	    </div>	    
        </div>            
        
	<div class="tab-pane fade" id="prServStatusHist" role="tabpanel" aria-labelledby="prServStatusHist-tab">
