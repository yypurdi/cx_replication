                    </tr><tr>
                        <td><%=resource.Subscript.label%></td>
                        <td><%=record.subscript%></td>						
                    </tr><tr>
                        <td><%=resource.OvwAccess.label%></td>
                        <td><%=record.ovw_access%></td>						
                    </tr><tr>
                        <td><%=resource.OvwAccPrd.label%></td>
                        <td><%=record.ovw_acc_prd%></td>						
                    </tr><tr>
                        <td><%=resource.Accessfee.label%></td>
                        <td><%=record.accessfee%></td>						
                    </tr><tr>
                        <td><%=resource.ChannelExcl.label%></td>
                        <td><%=record.channel_excl%></td>						
                    </tr><tr>
                        <td><%=resource.DisSubscr.label%></td>
                        <td><%=record.dis_subscr%></td>						
                    </tr><tr>
                        <td><%=resource.InstallDate.label%></td>
                        <td><%=dateFormat(record.install_date,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.TrialEndDate.label%></td>
                        <td><%=dateFormat(record.trial_end_date,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.PrmValueId.label%></td>
                        <td><%=record.prm_value_id%></td>						
                    </tr><tr>
                        <td><%=resource.Currency.label%></td>
                        <td><%=record.currency%></td>						
                    </tr><tr>
                        <td><%=resource.SrvType.label%></td>
                        <td><%=record.srv_type%></td>						
                    </tr><tr>
                        <td><%=resource.SrvSubtype.label%></td>
                        <td><%=record.srv_subtype%></td>						
                    </tr><tr>
                        <td><%=resource.OvwAdvCharge.label%></td>
                        <td><%=record.ovw_adv_charge%></td>						
                    </tr><tr>
                        <td><%=resource.AdvCharge.label%></td>
                        <td><%=record.adv_charge%></td>						
                    </tr><tr>
                        <td><%=resource.AdvChargePrd.label%></td>
                        <td><%=record.adv_charge_prd%></td>						
                    </tr><tr>
