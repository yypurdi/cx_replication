                    </tr><tr>
                        <td><%=resource.CdSeqno.label%></td>
                        <td><%=record.cd_seqno%></td>						
                    </tr><tr>
                        <td><%=resource.CoId.label%></td>
                        <td><%=record.co_id%></td>						
                    </tr><tr>
