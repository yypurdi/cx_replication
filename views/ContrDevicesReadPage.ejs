                <button type="submit" class="btn btn-default"><a href="/writeContrDevices?cdId=<%=record.cd_id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteContrDevices?cdId=<%=record.cd_id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
