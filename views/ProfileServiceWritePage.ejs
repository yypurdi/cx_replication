                <td><input type="text" class="form-control" name="statusHistno" value="<%=record.status_histno%>"/></td>
            </tr><tr>
                <td><%=resource.EntryDate.label%></td>
                <td><input type="text" class="form-control" name="entryDate" value="<%=record.entry_date%>"/></td>
            </tr><tr>
